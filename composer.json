{
    "name": "xolens/pglarapoll",
    "description": "Laravel survey repository package.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "xolens",
        "email": "xolens.official@gmail.com"
    }],
    "require": {
        "xolens/larapoll-contract": "dev-master",

        "xolens/pglarautil": "dev-master",
        "xolens/larautil-contract": "dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "~3.4.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "PgLarapoll\\PgLarapollServiceProvider"
            ]
        }
    },
    "repositories": [{
        "type": "path",
        "url": "/var/www/repositories/xolens.laravel/local/*"
    }]
}
